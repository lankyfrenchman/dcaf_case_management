$('#search_results').empty();
<% if @results.present? %>
  $("#search_results").append("<%= escape_javascript(render 'dashboards/table_content', title: 'Search results', table_type: 'search_results', pregnancies: @results ) %>");
<% else %>
  $("#search_results").append("<%= escape_javascript(render 'patients/new_patient', patient: @patient) %>");
<% end %>
